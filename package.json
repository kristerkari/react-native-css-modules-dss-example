{
  "name": "CSSModulesExample",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint . && stylelint 'src/**/*.@(css|scss)'",
    "web": "webpack-dev-server --mode development",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "postinstall": "cp -f .dss-webpack.patch ./node_modules/dss-webpack/index.js"
  },
  "dependencies": {
    "dss-classnames": "^0.1.0-beta.0",
    "es6-shim": "^0.35.3",
    "react": "16.4.1",
    "react-art": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-native": "0.56.0",
    "react-native-fontawesome": "^5.7.0",
    "react-native-web": "^0.8.8"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.53",
    "autoprefixer": "^8.6.5",
    "babel-jest": "^23.4.0",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-classnames": "^0.1.1",
    "babel-plugin-react-native-classname-to-style": "^1.2.1",
    "babel-plugin-react-native-platform-specific-extensions": "^1.1.1",
    "babel-preset-react": "^7.0.0-beta.3",
    "babel-preset-react-native": "5.0.2",
    "css-loader": "^1.0.0",
    "dss-webpack": "^0.1.0-beta.0",
    "eslint": "^5.1.0",
    "eslint-plugin-css-modules": "^2.7.5",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "23.4.1",
    "last-call-webpack-plugin": "^3.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "postcss": "^6.0.23",
    "postcss-loader": "^2.1.6",
    "react-hot-loader": "3.1.3",
    "react-native-css-transformer": "^1.2.2",
    "react-native-sass-transformer": "^1.2.2",
    "react-test-renderer": "16.4.1",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-react-native-css-modules": "^1.4.0",
    "stylelint-react-native": "^1.1.1",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11",
    "Android >= 4.4",
    "iOS >= 8"
  ],
  "jest": {
    "preset": "react-native"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": [
      "css-modules"
    ],
    "extends": [
      "plugin:css-modules/recommended"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-react-native-css-modules",
    "rules": {
      "selector-class-pattern": "^[a-z][a-zA-Z0-9]*$",
      "declaration-block-no-duplicate-properties": true,
      "no-duplicate-selectors": true,
      "no-extra-semicolons": true,
      "unit-whitelist": [
        "px",
        "rem",
        "deg",
        "%"
      ],
      "no-eol-whitespace": true,
      "no-missing-end-of-source-newline": true
    }
  }
}
